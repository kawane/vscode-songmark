{
    "name": "vscode-songmark",
    "displayName": "Songmark language support",
    "description": "Songmark language support for Visual Studio Code.",
    "repository": {
        "url": "https://github.com/kawane/vscode-songmark"
    },
    "version": "0.0.1",
    "publisher": "llgcode",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "songmark",
            "aliases": ["songmark", "songmark"],
            "extensions": [".song"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "songmark",
            "scopeName": "source.song",
            "path": "./syntaxes/songmark.tmLanguage.json"
        }]
    }
}